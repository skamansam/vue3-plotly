{
  "name": "vue3-plotly",
  "version": "1.1.0",
  "private": false,
  "scripts": {
    "dev": "vite ",
    "serve": "vite serve ./example/main.js --open",
    "build": "vite build --name vue-plotly --entry ./src/index.js --target lib",
    "test:unit": "vue-cli-service test:unit --coverage",
    "lint": "vue-cli-service lint --fix",
    "build:doc": "vue-cli-service build --entry ./example/main.js --dest docs --mode development",
    "prepublishOnly": "npm run lint && npm run build && npm run build:doc",
    "test:coverage": "vue-cli-service test:unit --coverage --verbose && codecov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/David-Desmaisons/vue-plotly.git"
  },
  "main": "dist/vue-plotly.umd.js",
  "module": "dist/vue-plotly.common.min.js",
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "src/*"
  ],
  "dependencies": {
    "core-js": "^3.24.1",
    "json-editor-vue": "^0.4.13",
    "plotly.js": "^2.14.0",
    "vue": "^3.2.37",
    "vue-resize-directive": "^1.2.0"
  },
  "devDependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/test-utils": "2.0.2",
    "@vuedoc/md": "4.0.0-beta5",
    "bootstrap": "^5.2.0",
    "codecov": "^3.8.3",
    "eslint": "^8.21.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "font-awesome": "^4.7.0",
    "ify-loader": "^1.1.0",
    "jest-canvas-mock": "^2.4.0",
    "less": "^4.1.3",
    "node-fetch": "^3.2.10",
    "node-stream": "^1.7.0",
    "stream-browserify": "^3.0.0",
    "util": "^0.12.4",
    "vite": "^3.0.7",
    "vue-draggable-resizable": "^2.3.0",
    "vue-json-editor": "^1.4.3",
    "vue-server-renderer": "^2.7.8"
  },
  "keywords": [
    "vue",
    "component"
  ],
  "license": "MIT"
}
